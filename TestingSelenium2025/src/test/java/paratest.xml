<?xml version="1.0" encoding="UTF-8"?>
<!--We can declareat suite as well as at test level, but we generally used at test level 
as it become easy for classes tag to find prameters tag at immidiate test tag -->
<suite parallel="false" name="Suite">
  <test name="Test">
  	<parameter name = "val1" value = "30" />
  	<parameter name = "val2" value = "10" />
  	<parameter name = "val3" value = "45" />
  	<parameter name = "val4" value = "20" />
  <!-- <parameter name = "message" value = "Welcome to manual testing" /> -->
    <classes>
      <class name="testng.ParamTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
